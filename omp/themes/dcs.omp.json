{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "bg": "#24283b",
    "bg_dark": "#1f2335",
    "bg_highlight": "#292e42",
    "fg": "#c0caf5",
    "fg_dark": "#a9b1d6",
    "comment": "#565f89",
    "blue": "#7aa2f7",
    "blue0": "#3d59a1",
    "blue1": "#2ac3de",
    "blue2": "#0db9d7",
    "cyan": "#7dcfff",
    "green": "#9ece6a",
    "green1": "#73daca",
    "green2": "#41a6b5",
    "magenta": "#bb9af7",
    "orange": "#ff9e64",
    "purple": "#9d7cd8",
    "red": "#f7768e",
    "red1": "#db4b4b",
    "teal": "#1abc9c",
    "yellow": "#e0af68",
    "terminal_black": "#414868",
    "git_add": "#449dab",
    "git_change": "#6183bb",
    "git_delete": "#914c54"
  },
  "blocks": [
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "time",
          "style": "diamond",
          "background": "p:bg_dark",
          "foreground": "p:teal",
          "trailing_diamond": "\ue0b8",
          "template": "\n<#9ece6a> \uf017</> {{ .CurrentDate | date \"<b>03</b>:04:05 <d>PM</d>\" }} ",
          "properties": {
            "time_format": "15:04:05"
          }
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "p:bg_dark",
          "foreground": "p:purple",
          "leading_diamond": "\ue0ba",
          "properties": {
            "threshold": 500,
            "style": "austin",
            "always_enabled": true
          },
          "style": "diamond",
          "template": " <#f69962>\udb80\udd09</> {{ .Ms }}<d><i>ms</i></d> \n",
          "type": "executiontime"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "segments": [
        {
          "background": "p:cyan",
          "foreground": "p:bg_dark",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b0",
          "template": "\uf135 ",
          "type": "text"
        },
        {
          "foreground": "p:bg_dark",
          "background": "p:cyan",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "template": "{{ if .SSHSession }} \uf0c1 {{ end }}",
          "type": "session"
        },
        {
          "background": "p:yellow",
          "foreground": "p:bg_dark",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "properties": {
            "folder_icon": "\uf115",
            "folder_separator_icon": " \ue0b1 ",
            "home_icon": "\ueb06",
            "style": "agnoster"
          },
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "p:fg",
          "foreground": "p:bg_dark",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "properties": {
            "branch_icon": "",
            "fetch_stash_count": true,
            "fetch_status": false,
            "fetch_upstream_icon": false
          },
          "template": " {{ .UpstreamIcon }}\u279c [{{ .HEAD }}] {{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }}  {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}  {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }}  {{ .StashCount }}{{ end }}",
          "type": "git"
        },
        {
          "background": "p:purple",
          "foreground": "p:fg",
          "trailing_diamond": "\ue0b0",
          "powerline_symbol": "\ue0b0",
          "style": "diamond",
          "template": " \u2665 ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 3
}