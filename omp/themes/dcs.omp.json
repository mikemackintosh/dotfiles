{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palettes": {
    "template": "{{ if eq .Env.DCSMODE \"storm\" }}storm{{else if eq .Env.DCSMODE \"day\" }}day{{else}}night{{ end }}",
    "list": {
      "night": {
        "bg": "#24283b",
        "bg_dark": "#1f2335",
        "bg_darkdark": "#1a1b26",
        "bg_highlight": "#292e42",
        "fg": "#c0caf5",
        "fg_dark": "#2c3148",
        "comment": "#565f89",
        "blue": "#7aa2f7",
        "blue0": "#24469b",
        "blue1": "#2ac3de",
        "blue2": "#0db9d7",
        "cyan": "#7dcfff",
        "green": "#9ece6a",
        "green1": "#73daca",
        "green2": "#41a6b5",
        "magenta": "#dd0155",
        "orange": "#fb8946",
        "purple": "#9d7cd8",
        "red": "#f7768e",
        "red1": "#db4b4b",
        "teal": "#1abc9c",
        "yellow": "#e0af68",
        "terminal_black": "#414868",
        "git_add": "#449dab",
        "git_change": "#6183bb",
        "git_delete": "#914c54"
      },
      "day": {
        "bg": "#e8e8e8",
        "bg_dark": "#a4a4ad",
        "bg_darkdark": "#c3c3c3",
        "bg_highlight": "#292e42",
        "fg": "#c0caf5",
        "fg_dark": "p:cteal",
        "comment": "#565f89",
        "blue": "#7aa2f7",
        "blue0": "#24469b",
        "blue1": "#2ac3de",
        "blue2": "#0db9d7",
        "cyan": "#7dcfff",
        "green": "#9ece6a",
        "green1": "#73daca",
        "green2": "#41a6b5",
        "magenta": "#dd0155",
        "orange": "#fb8946",
        "purple": "#9d7cd8",
        "red": "#f7768e",
        "red1": "#db4b4b",
        "teal": "p:bg_darkdark",
        "cteal": "#1abc9c",
        "yellow": "#e0af68",
        "terminal_black": "#414868",
        "git_add": "#449dab",
        "git_change": "#6183bb",
        "git_delete": "#914c54"
      },
      "storm": {
        "bg": "#24283b",
        "bg_dark": "#292e42",
        "bg_darkdark": "#1a1b26",
        "bg_highlight": "#292e42",
        "fg": "#c0caf5",
        "fg_dark": "#2c3148",
        "comment": "#565f89",
        "blue": "#7aa2f7",
        "blue0": "#24469b",
        "blue1": "#2ac3de",
        "blue2": "#0db9d7",
        "cyan": "#7dcfff",
        "green": "#9ece6a",
        "green1": "#73daca",
        "green2": "#41a6b5",
        "magenta": "#dd0155",
        "orange": "#fb8946",
        "purple": "#9d7cd8",
        "red": "#f7768e",
        "red1": "#db4b4b",
        "teal": "#1abc9c",
        "yellow": "#e0af68",
        "terminal_black": "#414868",
        "git_add": "#449dab",
        "git_change": "#6183bb",
        "git_delete": "#914c54"
      }
    }
  },
  "blocks": [
    {
      "alignment": "left",
      "newline": false,
      "segments": [
        {
          "type": "time",
          "style": "diamond",
          "alias": "timestamp",
          "background": "p:bg_dark",
          "foreground": "p:teal",
          "leading_diamond": "\n \ue0ba█",
          "trailing_diamond": "\ue0b8",
          "template": "{{ .CurrentDate | date \"<b>03</b>:04:05 <d>PM</d>\" }} ",
          "properties": {
            "time_format": "15:04:05"
          }
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "p:fg_dark",
          "background": "p:bg_dark",
          "leading_diamond": "\ue0ba",
          "style": "diamond",
          "template": " <p:fg_dark><b>/</b></>",
          "type": "text"
        },
        {
          "foreground": "p:fg",
          "background": "p:bg_dark",
          "leading_diamond": "\ue0ba",
          "properties": {
            "branch_icon": "",
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true
          },
          "style": "diamond",
          "template": " <p:magenta>\ue725</> <p:fg><b>{{ .HEAD }}</b></> {{ if .Staging.Changed }}<p:teal><d>staged:</d>{{.Staging.String}}</> {{end}}{{ if .Working.Changed }} <p:blue><b> {{.Working.Untracked}}</b></> <p:yellow> <b>{{.Working.Modified}}</b></> <p:green> <b>{{.Working.Added}}</b></> <p:red> <b>{{.Working.Deleted}}</b></> {{ end }}<p:fg_dark><b> ▍</b></>",
          "type": "git"
        },
        {
          "background": "p:bg_dark",
          "foreground": "p:purple",
          "properties": {
            "threshold": 500,
            "style": "austin",
            "always_enabled": true
          },
          "style": "diamond",
          "template": " <#f69962>\udb80\udd09</> {{ .Ms }}<d><i>ms</i></d> \n",
          "type": "executiontime"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "segments": [
        {
          "background": "p:teal",
          "foreground": "p:bg_dark",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b0",
          "background_templates": [
            "{{ if .Error }}p:red{{ end }}"
          ],
          "template": " \uf135 ",
            "properties": {
            "always_enabled": true
            },
          "type": "status"
        },
        {
          "foreground": "p:bg_dark",
          "background": "p:purple",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "template": "{{ if .SSHSession }} \uf0c1 {{ end }}",
          "type": "session"
        },
        {
          "background": "p:fg_dark",
          "foreground": "p:yellow",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "properties": {
            "folder_icon": "..",
            "folder_separator_icon": " / <d>\uf07c</d> ",
            "home_icon": "\uf015",
            "max_depth": 3,
            "cycle_folder_separator": true,
            "cycle": [
              "p:yellow",
              "p:green",
              "p:blue2",
              "p:blue1",
              "p:purple"
            ],
            "folder_format": "<d>%s</d>",
            "left_format": "<b>%s</b> ",
            "right_format": "<b>%s</b>",
            "style": "agnoster_short"
          },
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "p:bg_dark",
          "foreground": "p:fg_dark",
          "style": "diamond",
          "template": " / ",
          "type": "text",
          "leading_diamond": "<transparent>\ue0b0</transparent>\ue0b6",
          "properties": {
            "always_enabled": true
          }
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "p:red",
          "style": "plain",
          "template": "\u0021",
          "type": "root"
        },
        {
          "foreground": "p:orange",
          "style": "plain",
          "template": "\u276f",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "transient_prompt": {
    "foreground": "p:yellow",
    "style": "plain",
    "template": "<d>{{index .Segments.timestamp.Segment \"Text\" | }}\n\u276f</d> ",
    "type": "text"
  },
  "console_title_template": "{{ .Folder }}",
  "final_space": true,
  "version": 3
}
